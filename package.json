    "@fortawesome/react-fontawesome": "0.2.2",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-css-only": "3.3.5",
    "bootstrap3-native": "1.0.11",
    "date-fns": "^3.6.0",
    "node-noop": "1.0.0",
    "object-assign": "4.1.1",
    "prop-types": "^15.8.1",
    "query-string": "6.4.2",
    "react": "18.3.1",
    "react-bootstrap": "2.10.5",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "react-simple-animate": "3.5.2",
