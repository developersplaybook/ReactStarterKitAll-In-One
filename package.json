{
  "name": "react-starterkitall-in-one",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint ClientApp/**/*.{js,jsx}",
    "clean": "rimraf ./ClientApp/dist ./wwwroot/dist",
    "buildvendor:dev": "webpack --config webpack.config.vendor.js --env development",
    "buildvendor:prod": "webpack --config webpack.config.vendor.js --env production",
    "build:dev": "npm run clean && npm run buildvendor:dev && webpack --config webpack.config.js --env development",
    "build:prod": "npm run clean && npm run buildvendor:prod && webpack --config webpack.config.js --env production"
  },
  "dependencies": {
