    "babel-preset-react": "7.0.0-beta.3",
    "babel-preset-stage-0": "7.0.0-beta.3",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.0",
